# .env.example

# Port your server will listen on
PORT=3000

# PostgreSQL connection string (replace placeholders with actual values)
DATABASE_URL=postgresql://username:password@localhost:5432/clubroom

# Secret used to sign session cookies (set a strong random string)
SESSION_SECRET=your_session_secret_here

# Secret passcode for users to join the club (members only)
MEMBER_PASSCODE=your_member_passcode_here

# Secret passcode for admin users (higher privileges)
ADMIN_PASSCODE=your_admin_passcode_here